{
    "name": "twig",
    "displayName": "Twig",
    "description": "Syntax highlighting for PHP Twig",
    "icon": "icon.svg",
    "version": "1.0.2",
    "publisher": "whatwedo",
    "engines": {
        "vscode": "^0.10.1"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/whatwedo/vscode-twig/issues",
        "email": "welove+vscode@whatwedo.ch"
    },
    "homepage": "https://github.com/whatwedo/vscode-twig",
    "repository": {
        "type": "git",
        "url": "https://github.com/whatwedo/vscode-twig.git"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "php",
        "twig",
        "html",
        "template"
    ],
    "galleryBanner": {
        "color": "#BBCE29",
        "theme": "light"
    },
    "contributes": {
        "languages": [
            {
                "id": "twig",
                "aliases": [
                    "HTML (Twig)",
                    "twig"
                ],
                "extensions": [
                    ".twig",
                    ".html.twig"
                ],
                "configuration": "./twig.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "twig",
                "scopeName": "text.html.twig",
                "path": "./syntaxes/twig.tmLanguage"
            }
        ]
    }
}